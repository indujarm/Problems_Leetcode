https://leetcode.com/problems/container-with-most-water/

class Solution {
public:
    int maxArea(vector<int>& h) {
        int i =0;
        int j = h.size()-1;
        int maxArea = 0;
        while(i<j)
        {
            maxArea =max(maxArea, (j-i)*(min(h[i],h[j])));
            if(h[i]<h[j])
            i++;
            else
            j--;
        }
        return maxArea;

        
    }
};
